<?xml version="1.0" encoding="UTF-8"?>
<solver xmlns="https://timefold.ai/xsd/solver"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://timefold.ai/xsd/solver https://timefold.ai/xsd/solver/solver.xsd"
>

    <environmentMode>STEP_ASSERT</environmentMode>
    <randomSeed>42</randomSeed>

    <solutionClass>zzk.townshipscheduler.backend.scheduling.model.TownshipSchedulingProblem</solutionClass>
    <!--    <entityClass>zzk.townshipscheduler.backend.scheduling.model.SchedulingTypeQueueFactoryInstance</entityClass>-->
    <entityClass>zzk.townshipscheduler.backend.scheduling.model.SchedulingTypeSlotFactoryInstance</entityClass>
    <entityClass>zzk.townshipscheduler.backend.scheduling.model.SchedulingFactoryQueueProducingArrangement</entityClass>
    <entityClass>zzk.townshipscheduler.backend.scheduling.model.SchedulingFactorySlotProducingArrangement</entityClass>

    <scoreDirectorFactory>
        <constraintProviderClass>
            zzk.townshipscheduler.backend.scheduling.score.TownshipSchedulingConstraintProvider
        </constraintProviderClass>
    </scoreDirectorFactory>

</solver>
