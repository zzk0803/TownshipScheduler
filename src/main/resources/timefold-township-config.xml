    <localSearch>
        <unionMoveSelector>
            <changeMoveSelector>
                <filterClass>
                    zzk.townshipscheduler.backend.scheduling.algorithm.SlotProducingArrangementPlanningFactoryChangeMoveSelectionFilter
                </filterClass>
                <entitySelector id="localSearch_changeMoveSelector_es_TypeSlot_planningFactory">
                    <entityClass>
                        zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeSlot
                    </entityClass>
                </entitySelector>
                <valueSelector variableName="planningFactory">
                </valueSelector>
            </changeMoveSelector>

            <unionMoveSelector>
                <fixedProbabilityWeight>3.0</fixedProbabilityWeight>
                <changeMoveSelector>
                    <entitySelector id="localSearch_changeMoveSelector_es_TypeSlot_planningDateTimeSlot">
                        <entityClass>
                            zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeSlot
                        </entityClass>
                    </entitySelector>
                    <valueSelector id="localSearch_changeMoveSelector_vs_TypeSlot_planningDateTimeSlot"
                                   variableName="planningDateTimeSlot">
                    </valueSelector>
                </changeMoveSelector>

                <changeMoveSelector>
                    <entitySelector id="localSearch_changeMoveSelector_es_TypeQueue_planningDateTimeSlot">
                        <entityClass>
                            zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeQueue
                        </entityClass>
                    </entitySelector>
                    <valueSelector id="localSearch_changeMoveSelector_vs__planningDateTimeSlot"
                                   variableName="planningDateTimeSlot">
                    </valueSelector>
                </changeMoveSelector>
            </unionMoveSelector>

            <tailChainSwapMoveSelector>
                <filterClass>
                    zzk.townshipscheduler.backend.scheduling.algorithm.QueueProducingArrangementPlanningFactoryTailChainSwapMoveSelectionFilter
                </filterClass>
                <entitySelector id="localSearch_tailChainSwapMoveSelector_es_TypeQueue_planningDateTimeSlot">
                    <entityClass>
                        zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeQueue
                    </entityClass>
                </entitySelector>
                <valueSelector id="localSearch_tailChainSwapMoveSelector_vs_TypeQueue_planningDateTimeSlot+"
                               variableName="planningPreviousProducingArrangementOrFactory"/>
            </tailChainSwapMoveSelector>

            <subChainSwapMoveSelector>
                <filterClass>
                    zzk.townshipscheduler.backend.scheduling.algorithm.QueueProducingArrangementPlanningFactorySubChainSwapMoveSelectionFilter
                </filterClass>
                <entityClass>
                    zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeQueue
                </entityClass>
                <subChainSelector>
                    <valueSelector variableName="planningPreviousProducingArrangementOrFactory"/>
                </subChainSelector>
                <!--ai.timefold.solver.core.impl.heuristic.selector.move.generic.chained.SubChainReversingSwapMove-->
                <selectReversingMoveToo>true</selectReversingMoveToo>
            </subChainSwapMoveSelector>

            <subChainChangeMoveSelector>
                <filterClass>
                    zzk.townshipscheduler.backend.scheduling.algorithm.QueueProducingArrangementPlanningFactorySubChainChangeMoveSelectionFilter
                </filterClass>
                <entityClass>
                    zzk.townshipscheduler.backend.scheduling.model.SchedulingProducingArrangementFactoryTypeQueue
                </entityClass>
                <subChainSelector>
                    <valueSelector variableName="planningPreviousProducingArrangementOrFactory"/>
                    <minimumSubChainSize>1</minimumSubChainSize>
                </subChainSelector>
                <valueSelector variableName="planningPreviousProducingArrangementOrFactory"/>
                <!--ai.timefold.solver.core.impl.heuristic.selector.move.generic.chained.SubChainReversingChangeMove-->
                <selectReversingMoveToo>true</selectReversingMoveToo>
            </subChainChangeMoveSelector>

        </unionMoveSelector>

        <acceptor>
            <entityTabuSize>7</entityTabuSize>
            <valueTabuSize>7</valueTabuSize>
            <moveTabuSize>7</moveTabuSize>
            <undoMoveTabuSize>7</undoMoveTabuSize>
            <simulatedAnnealingStartingTemperature>[100/500/100]hard/[10/1]soft</simulatedAnnealingStartingTemperature>
            <!--            <greatDelugeWaterLevelIncrementRatio>0.0000000618</greatDelugeWaterLevelIncrementRatio>-->
            <!--            <lateAcceptanceSize>4096</lateAcceptanceSize>-->
        </acceptor>

        <forager>
            <acceptedCountLimit>1</acceptedCountLimit>
            <finalistPodiumType>STRATEGIC_OSCILLATION_BY_LEVEL_ON_BEST_SCORE</finalistPodiumType>
        </forager>
    </localSearch>

</solver>
